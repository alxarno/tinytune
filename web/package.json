{
  "name": "tinytune",
  "version": "1.0.0",
  "description": "Frontend of tiny media server",
  "main": "index.js",
  "scripts": {
    "build-vendor": "webpack --config webpack.vendor.config --mode='production'",
    "build": "webpack --config webpack.config.js --mode='production'",
    "build-all": "npm run build-vendor && npm run build",
    "watch": "npm run build-vendor && webpack --watch --config webpack.config.js --mode='development'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alxarno/tinytune.git"
  },
  "keywords": [
    "HTMX",
    "Bootstrap",
    "VideoJS"
  ],
  "author": "alxarno",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/alxarno/tinytune/issues"
  },
  "homepage": "https://github.com/alxarno/tinytune#readme",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "fslightbox": "^3.4.2",
    "hls.js": "^1.5.17",
    "htmx.org": "^2.0.3",
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "file-loader": "^6.2.0",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  }
}
